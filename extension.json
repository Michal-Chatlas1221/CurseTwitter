{
	"name": "Curse Twitter",
	"version": "1.4.1",
	"author": [
		"Hydra Wiki Platform Team"
	],
	"url": "https://gitlab.com/HydraWiki/extensions/CurseTwitter",
	"descriptionmsg": "cursetwitter_description",
	"license-name": "GPL-3.0-only",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.29.0"
	},
	"MessagesDirs": {
		"CurseTwitter": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"CurseTwitterHooks": "CurseTwitter.hooks.php",
		"CurseTwitter": "classes/CurseTwitter.php"
	},
	"ResourceModules": {
		"ext.curse.twitter": {
			"styles": [
				"css/ext.curse.twitter.css"
			],
			"position": "top"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "CurseTwitter"
	},
	"Hooks": {
		"ParserFirstCallInit": "CurseTwitterHooks::onParserFirstCallInit"
	},
	"manifest_version": 2
}
